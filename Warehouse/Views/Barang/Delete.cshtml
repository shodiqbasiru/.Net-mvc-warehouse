@model Warehouse.Models.Barang

<h1>Delete Barang</h1>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>@Model.NamaBarang</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Kode Barang</dt>
        <dd class="col-sm-10">@Model.KodeBarang</dd>

        <dt class="col-sm-2">Nama Barang</dt>
        <dd class="col-sm-10">@Model.NamaBarang</dd>

        <dt class="col-sm-2">Harga Barang</dt>
        <dd class="col-sm-10">@Model.HargaBarang</dd>

        <dt class="col-sm-2">Jumlah Barang</dt>
        <dd class="col-sm-10">@Model.JumlahBarang</dd>

        <dt class="col-sm-2">Expired Date</dt>
        <dd class="col-sm-10">@Model.ExpiredDate</dd>

        <dt class="col-sm-2">Gudang</dt>
        <dd class="col-sm-10">@Model.Gudang.NamaGudang</dd>
    </dl>

    <form method="post" asp-action="DeleteConfirmed" asp-controller="Barang">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Back to List</a>
    </form>
</div>